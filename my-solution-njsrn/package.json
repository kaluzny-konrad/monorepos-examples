{
  "name": "my-solution-njsrn",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "type-check": "turbo type-check",
    "clean": "turbo clean && rimraf node_modules",
    "dev:web": "turbo dev --filter=@repo/web",
    "dev:native": "turbo dev --filter=@repo/native"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "turbo": "^2.7.0",
    "typescript": "5.9.3"
  },
  "pnpm": {
    "overrides": {
      "rimraf": "^6.1.2",
      "glob": "^13.0.0"
    }
  },
  "packageManager": "pnpm@10.26.1",
  "engines": {
    "node": ">=18"
  }
}
